 /**app.wxss**/
 page{
  background: #fff;
  height: 100%;
 }
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


/* base css */
.clearfix::after{
  content: "";
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}
.clear{
  zoom: 1;
}

/* 导航 */
.huawarp{
  margin-top: 10px;
}
.flowerName{
  float: left;
  border: 2rpx solid #e5e5e5;
  width: 100rpx;
  line-height:30px; 
}

.subtitle{
  margin-bottom: 10px;
  font-size: 32rpx;
  font-weight: 600;
  margin-left: 12rpx;
}
.hualist{
  padding-left: 4px;
  font-size: 12px;
  background: #d1d1d1;
  box-shadow: 0 0 2px rgb(230, 228, 228);
}


/* 底部保持固定在页面底部 */
.fixbox{
  height: 100%;
  width: 100%;
  display: flex;
  flex-flow: column;
  justify-content: space-between;
}
